  margin-left: 50px;
}

li img {
  float: left;
  margin: 0 20px 0 0;
}

li p {
  margin-left: 50px;
  font: 200 17px/1.5 Georgia, Times New Roman, serif;
}


li:hover {
  background: #eee;
  cursor: pointer;
}
